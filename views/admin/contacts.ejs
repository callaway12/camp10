<% include ../header.ejs %>
    <table class="table table-bordered table-hover">
        <tr>
            <th>제목</th>
            <th>작성일</th>
            <th>삭제</th>
        </tr>
        <tr>
        <%contacts.forEach(function(contact){%> 

        
            <td>
                <a href="/admin/contacts/detail/<%=contact.id%>">
                <%=contact.name%>
            </td>
            <td>
                <%=contact.getDate.year%> -
                <%=contact.getDate.month%> -
                <%=contact.getDate.day%> 
            </td>
            <td>
	            <a href="/admin/contact/delete/<%=contact.id%>" class="btn btn-danger" onclick="return confirm('삭제하시겠습니까?')">삭제</a>
            </td> <%//이거 제이쿼리로 밑에 빼는거 연습해보자!!%>
        </tr>
        <%})%>


    </table>

    <a href="/admin/contact/write" class="btn btn-default">작성하기</a>

<% include ../footer.ejs %>